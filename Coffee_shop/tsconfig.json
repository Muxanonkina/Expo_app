{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".", // Добавьте эту строку
    "paths": {
      "@/*": ["./*"],
      "@assets/*": ["assets/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.tsx",
    "constants/MenuImages.ts",
    "constants/MenuImages.ts",
    "app/menu.tsx",
    "constants/MenuItems.ts"
  ]
}
